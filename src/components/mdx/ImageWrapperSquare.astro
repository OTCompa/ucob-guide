---
import ImageWrapper from './ImageWrapper.astro';
import clsx from 'clsx';

const props = Astro.props;

interface Props {
  img: ImageMetadata;
  alt?: string;
  classdiv?: string;
  classimg?: string;
}
---

<ImageWrapper img={props.img} alt={props.alt} classdiv={props.classdiv} classimg={clsx("imagewrapper-square", props.classimg)}/>